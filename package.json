{
    "name": "feedboard",
    "displayName": "feedboard",
    "description": "A hello world sample extension to demonstrate using React + Vite with the Webview UI Toolkit for VS Code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.75.0"
    },
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "feedboard-sidebar-view",
                    "title": "feedboard",
                    "icon": "media/checklist.svg"
                }
            ]
        },
        "views": {
            "feedboard-sidebar-view": [
                {
                    "type": "webview",
                    "id": "feedboard-main-view",
                    "name": "feedboard",
                    "icon": "media/checklist.svg",
                    "contextualTitle": "feedboard"
                }
            ]
        },
        "commands": [
            {
                "command": "feedboard.main-view",
                "title": "Hello World (React + Vite): Show"
            }
        ]
    },
    "scripts": {
        "install:all": "yarn && cd webview-ui && yarn",
        "start:ui": "cd webview-ui && yarn start",
        "build:ui": "cd webview-ui && yarn build",
        "vscode:prepublish": "yarn compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn compile && yarn lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.19.0",
        "glob": "^7.1.6",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3",
        "vscode-test": "^1.5.0"
    }
}
